                        alignItems="center"
                        flexDirection="column"
                        background="#FF018786"
                        justifyContent="flexStart">
                        <Text
                            width="80"
                            height="20"
                            key="Text"
                            textSize="13"
                            textStyle="bold"
                            text="月定开榜单"></Text>
                        <Text
                            width="80"
                            height="20"
                            key="Text"
                            textSize="11"
                            text="业绩比较基准平均3.0%"></Text>
                        <Text
                            width="80"
                            height="20"
                            key="Text"
                            textSize="13"
                            textStyle="bold"
                            marginTop="5"
                            text="灵活申赎榜单"></Text>
